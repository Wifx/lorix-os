From a25ca361ef13fec6d49cbf0a4830dd13809fc782 Mon Sep 17 00:00:00 2001
From: Yannick Lanz <yannick.lanz@wifx.net>
Date: Thu, 13 Oct 2022 15:14:56 +0200
Subject: [PATCH 6/6] wifx l1/lorix: remove warning about dummy vcc regulator
 in dts

Signed-off-by: Yannick Lanz <yannick.lanz@wifx.net>
---
 arch/arm/boot/dts/l1.dts              | 11 +++++++++++
 arch/arm/boot/dts/lorix-one-base.dtsi |  4 +++-
 2 files changed, 14 insertions(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/l1.dts b/arch/arm/boot/dts/l1.dts
index c6189a12b163..03cbfdae4c0c 100644
--- a/arch/arm/boot/dts/l1.dts
+++ b/arch/arm/boot/dts/l1.dts
@@ -111,12 +111,14 @@ m24c08: eeprom@54 {
 					reg = <0x54>;
 					pagesize = <16>;
 					status = "okay";
+					vcc-supply = <&vcc_3v3_reg>;
 				};
 
 				at24mac402: mac_eeprom@58 {
 					compatible = "atmel,24mac402";
 					reg = <0x58>;
 					status = "okay";
+					vcc-supply = <&vcc_3v3_reg>;
 				};
 
 				ds1339: rtc@68 {
@@ -384,6 +386,15 @@ status_internal {
 		};
 	};
 
+	vcc_3v3_reg: fixedregulator_3v3 {
+		compatible = "regulator-fixed";
+		regulator-name = "VCC 3V3";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+
 	vddbu_2v_reg: fixedregulator@0 {
 		compatible = "regulator-fixed";
 		regulator-name = "VDDBU 2V";
diff --git a/arch/arm/boot/dts/lorix-one-base.dtsi b/arch/arm/boot/dts/lorix-one-base.dtsi
index 603959dc615c..35f38dddc56c 100644
--- a/arch/arm/boot/dts/lorix-one-base.dtsi
+++ b/arch/arm/boot/dts/lorix-one-base.dtsi
@@ -102,12 +102,14 @@ temp_sensor: lm75@4d {
 					compatible = "lm75";
 					reg = <0x4d>;
 					status = "okay";
+					vs-supply = <&vcc_3v3_reg>;
 				};
 
 				i2c_eeprom: i2c_eeprom@58 {
 					compatible = "atmel,24mac402";
 					reg = <0x58>;
 					status = "okay";
+					vcc-supply = <&vcc_3v3_reg>;
 				};
 			};
 
@@ -277,7 +279,7 @@ sysled {
 		};
 	};
 
-    vcc_3v3_reg: fixedregulator_3v3 {
+	vcc_3v3_reg: fixedregulator_3v3 {
 		compatible = "regulator-fixed";
 		regulator-name = "VCC 3V3";
 		regulator-min-microvolt = <3300000>;
-- 
2.34.1


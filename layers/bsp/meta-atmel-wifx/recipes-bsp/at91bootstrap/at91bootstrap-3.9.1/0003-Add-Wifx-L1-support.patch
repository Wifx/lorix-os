+  pmc_enable_system_clock(AT91C_PMC_SMDCK);
+  pmc_set_smd_clock_divider(AT91C_PMC_SMDDIV);
+  pmc_enable_periph_clock(AT91C_ID_SMD);
+  writel(0xF, (0x0C + AT91C_BASE_SMD));
+  pmc_disable_periph_clock(AT91C_ID_SMD);
